<h2>Sign up!</h2>

<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Submit"%>
  <br>
<% end %>

<p>
<%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>
</p>
